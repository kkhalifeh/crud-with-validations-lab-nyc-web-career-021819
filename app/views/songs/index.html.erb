<h1>Songs#index</h1>

<div><%= link_to 'Create Song', new_song_path %></div>
<br />


<table style="width:50%">
  <tr>
    <th>Song Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>
  <% @songs.each do |song| %>
  <tbody>
  <tr>
    <td><%= link_to song.title, song_path(song) %></td>
    <td><%= song.released %></td>
    <td><%= song.release_year %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
    <td>
      <%= link_to 'View', song %> |
      <%= link_to 'Edit', edit_song_path(song) %> |
      <%= link_to 'Delete', song, method: :delete, data: { confirm: 'Confirm delete?' } %>
    </td>
      <% end %>
  </tr>
</tbody>
</table>
